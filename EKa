    nbytes += *ip;
  }
    
 нфл = 0;
  для (l = свободный_list_begin(); l != NULL; л = свободный_list_next(l)) {
    ip = (int *) l;
    принтф("Ошибка: Общее количество выделенных байтов и на свободный список = %d, а не %d\n", nbytes, tbytes);
    выход(0);
  }
    
